Content-Type: application/json

{
  "eventDefinitionId": "critical-alert-001",
  "eventDefinitionType": "aggregation-v1",
  "eventDefinitionTitle": "系統服務停止",
  "eventDefinitionDescription": "關鍵服務意外停止",
